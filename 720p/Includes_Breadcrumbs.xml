        <textcolor>bclabel</textcolor>
        <scroll>false</scroll>
        <animation effect="slide" start="0,0" end="0,-6" time="0" condition="true">Conditional</animation>
      </control>
    </control>
  </include>
  <include name="Breadcrumbs">
    <control type="group">
      <include>Animation_HideBreadCrumbs</include>
      <control type="image">
        <posx>0</posx>
        <posy>-36</posy>
        <width>1280</width>
        <height>79</height>
        <texture flipy="true">home/glass_bar.png</texture>
        <colordiffuse>bcbardiffuse</colordiffuse>
      </control>
      <include>Neon_Topbar</include>
      <control type="group">
        <visible>!Player.HasMedia|!IsEmpty(Window(videolibrary).Property(tvtunesIsAlive))</visible>
        <control type="grouplist">
          <posx>15</posx>
          <posy>0</posy>
          <width>500</width>
          <align>left</align>
          <visible></visible>
          <itemgap>0</itemgap>
          <orientation>horizontal</orientation>
          <include condition="Window.IsActive(videolibrary)">Breadcrumbs_Videos</include>
          <include condition="Window.IsActive(musiclibrary)">Breadcrumbs_MusicLibrary</include>
          <include condition="Window.IsActive(musicfiles)">Breadcrumbs_MusicFiles</include>
          <include condition="Window.IsActive(pictures)">Breadcrumbs_Pictures</include>
          <include condition="Window.IsActive(programs)">Breadcrumbs_Programs</include>
          <include condition="Window.IsActive(addonbrowser)">Breadcrumbs_Addonbrowser</include>
          <include condition="Window.IsActive(weather)">Breadcrumbs_Weather</include>
          <include condition="Window.IsActive(settings)|Window.IsActive(12)|Window.IsActive(13)|Window.IsActive(14)|Window.IsActive(15)|Window.IsActive(16)|Window.IsActive(17)|Window.IsActive(18)|Window.IsActive(19)|Window.IsActive(systeminfo)|Window.IsActive(1111)">Breadcrumbs_Settings</include>
          <include condition="Window.IsActive(videoplaylist)">Breadcrumbs_Videoplaylist</include>
          <include condition="Window.IsActive(musicplaylisteditor)">Breadcrumbs_Musicplaylisteditor</include>
          <include condition="Window.IsActive(filemanager)">Breadcrumbs_Filemanager</include>
          <include condition="Window.IsActive(script-NextAired-TVGuide.xml)">Breadcrumbs_NextAired</include>
          <control type="label">
            <include>BreadcrumbsLabel</include>
            <label>$INFO[Container.FolderName, / ]</label>
            <include>BreadcrumbsLabel</include>
          </control>
        </control>
      </control>
      <control type="grouplist">
        <posx>0</posx>
        <posy>6</posy>
        <width>1280</width>
        <align>center</align>
        <aligny>center</aligny>
        <itemgap>10</itemgap>
        <visible>!IsEmpty(Container.NumItems)</visible>
        <orientation>horizontal</orientation>
        <animation effect="fade" time="400" delay="400">Visible</animation>
        <animation effect="fade" time="400">Hidden</animation>
        <control type="label">
          <posx>0</posx>
          <posy>0</posy>
          <height>25</height>
          <aligny>center</aligny>
          <width min="10" max="1280">auto</width>
          <font>Font_Reg17</font>
          <label>$VAR[BreadcrumbsTrailerVar]</label>
          <textcolor>bclabel</textcolor>
          <scroll>false</scroll>
          <animation effect="slide" start="0,0" end="0,-6" time="0" condition="true">Conditional</animation>
          <visible>!IsEmpty(ListItem.Trailer)</visible>
        </control>
        <control type="image">
          <description>Items Icon</description>
          <posx>0</posx>
          <posy>0</posy>
          <width>20</width>
          <height>15</height>
          <aspectratio>stretch</aspectratio>
          <texture>breadcrumbs/items.png</texture>
          <colordiffuse>bcicon</colordiffuse>
        </control>
        <control type="label">
          <posx>0</posx>
          <posy>0</posy>
          <height>25</height>
          <aligny>center</aligny>
          <width min="10" max="1280">auto</width>
          <font>Font_Reg17</font>
          <label>$INFO[Container.NumItems] $VAR[ItemLabelVar]</label>
          <textcolor>bclabel</textcolor>
          <scroll>false</scroll>
          <animation effect="slide" start="0,0" end="0,-6" time="0" condition="true">Conditional</animation>
        </control>
        <control type="image">
          <description>Items Icon</description>
          <posx>0</posx>
          <posy>0</posy>
          <width>20</width>
          <height>15</height>
          <aspectratio>stretch</aspectratio>
          <texture>breadcrumbs/pages.png</texture>
          <colordiffuse>bcicon</colordiffuse>
          <animation effect="fade" time="400" delay="400">Visible</animation>
          <animation effect="fade" time="400">Hidden</animation>
        </control>
        <control type="label">
          <posx>0</posx>
          <posy>0</posy>
          <height>25</height>
          <aligny>center</aligny>
          <width min="10" max="1280">auto</width>
          <font>Font_Reg17</font>
          <label>$INFO[Container.CurrentPage,$LOCALIZE[31024] ]$INFO[Container.NumPages,/]</label>
          <textcolor>bclabel</textcolor>
          <scroll>false</scroll>
          <align>left</align>
          <animation effect="slide" start="0,0" end="0,-6" time="0" condition="true">Conditional</animation>
        </control>
      </control>
      <control type="group">
        <description>Clock + Date and Time</description>
        <animation effect="slide" start="0" end="-40" time="200" tween="quadratic" easing="out" condition="Window.IsActive(mutebug)">Conditional</animation>
        <control type="grouplist">
          <visible>true</visible>
          <posx>0</posx>
          <posy>0</posy>
          <width>1255</width>
          <align>right</align>
          <aligny>center</aligny>
          <orientation>horizontal</orientation>
          <control type="label">
            <posx>0</posx>
            <posy>0</posy>
            <height>25</height>
            <aligny>center</aligny>
            <width min="10" max="1280">auto</width>
            <font>Font_Reg17</font>
            <textcolor>bclabel</textcolor>
            <label>[COLOR bclabel]$INFO[System.Date]  |  [/COLOR][COLOR=time]$INFO[System.Time][/COLOR]       </label>
          </control>
        </control>
        <control type="group">
          <!--clock-->
          <posx>1240</posx>
          <posy>0</posy>
          <width>26</width>
          <height>26</height>
          <control type="image">
            <description>Clock Icon</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>26</width>
            <height>26</height>
            <colordiffuse>clock</colordiffuse>
            <texture>breadcrumbs/clock/clock_panel.png</texture>
          </control>
          <control type="image">
            <description>Clock Second</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>26</width>
            <height>26</height>
            <colordiffuse>clock</colordiffuse>
            <texture>breadcrumbs/clock/seconds/$INFO[System.Time(ss)].png</texture>
          </control>
          <control type="image">
            <description>Clock Minute</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>26</width>
            <height>26</height>
            <colordiffuse>clock</colordiffuse>
            <texture>breadcrumbs/clock/minutes/$INFO[System.Time(mm)].png</texture>
          </control>
          <control type="image">
            <description>Clock Hour</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>26</width>
            <height>26</height>
            <colordiffuse>clock</colordiffuse>
            <texture>breadcrumbs/clock/hours/$INFO[System.Time(hh)].png</texture>
          </control>
          <control type="image">
            <description>Clock Cap</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>26</width>
            <height>26</height>
            <colordiffuse>clock</colordiffuse>
            <texture>breadcrumbs/clock/clock_cap.png</texture>
          </control>
        </control>
      </control>
    </control>
  </include>
</includes>